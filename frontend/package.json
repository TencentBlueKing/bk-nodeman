{
  "name": "nodeman",
  "version": "1.0.0",
  "description": "Project description",
  "author": "",
  "repository": {},
  "scripts": {
    "dev": "better-npm-run dev",
    "clean": "rimraf dist",
    "clean-dll": "rimraf static/lib*",
    "build": "better-npm-run build",
    "build:analyzer": "better-npm-run build:analyzer",
    "dll": "better-npm-run dll",
    "lint": "eslint --ext .ts,.js,.vue src/",
    "fix": "eslint --fix --ext .ts,.js,.vue src/",
    "check-dll": "ts-node --project build/tsconfig.json build/check-dll.ts"
  },
  "keywords": [],
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.(vue|ts)": [
      "eslint --fix",
      "git add"
    ]
  },
  "betterScripts": {
    "dll": {
      "command": "ts-node --project build/tsconfig.json build/build-dll.ts"
    },
    "dev": {
      "command": "ts-node --project build/tsconfig.json build/dev-server.ts",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "npm run clean && npm run clean-dll && npm run dll && ts-node --project build/tsconfig.json build/build.ts",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:analyzer": {
      "command": "npm run clean && npm run clean-dll && npm run dll && ts-node --project build/tsconfig.json build/build.ts",
      "env": {
        "NODE_ENV": "production",
        "npm_config_report": 1
      }
    }
  },
